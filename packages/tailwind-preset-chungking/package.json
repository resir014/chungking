{
  "name": "@resir014/tailwind-preset-chungking",
  "version": "0.1.4-canary.5",
  "description": "TailwindCSS preset for the Chungking design system.",
  "source": "src/tailwind-preset-chungking.js",
  "main": "dist/tailwind-preset-chungking.js",
  "module": "dist/esm/tailwind-preset-chungking.js",
  "types": "dist/tailwind-preset-chungking.d.ts",
  "repository": "https://github.com/resir014/chungking.git",
  "author": "Resi Respati <resir014@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup-node --legacy-output",
    "clean": "rimraf dist",
    "dev": "tsup-node --watch --legacy-output",
    "prepublish": "yarn clean && yarn build",
    "validate": "yarn lint"
  },
  "dependencies": {
    "@resir014/chungking-core": "^0.1.2-canary.5"
  },
  "peerDependencies": {
    "tailwindcss": "^2.0.0"
  },
  "tsup": {
    "target": "node16",
    "entryPoints": [
      "src/tailwind-preset-chungking.js"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true
  }
}
