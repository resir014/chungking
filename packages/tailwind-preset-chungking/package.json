{
  "name": "@resir014/tailwind-preset-chungking",
  "version": "0.1.4-canary.3",
  "description": "TailwindCSS preset for the Chungking design system.",
  "type": "module",
  "source": "src/tailwind-preset-chungking.js",
  "types": "dist/tailwind-preset-chungking.d.ts",
  "exports": {
    ".": {
      "import": "./dist/tailwind-preset-chungking.js",
      "require": "./dist/tailwind-preset-chungking.cjs"
    }
  },
  "repository": "https://github.com/resir014/chungking.git",
  "author": "Resi Respati <resir014@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup-node",
    "clean": "rimraf dist",
    "prepublishOnly": "yarn clean && yarn build",
    "validate": "yarn lint"
  },
  "dependencies": {
    "@resir014/chungking-core": "^0.1.2-canary.3"
  },
  "peerDependencies": {
    "tailwindcss": "^2.0.0"
  },
  "devDependencies": {},
  "tsup": {
    "target": "node16",
    "entryPoints": [
      "src/tailwind-preset-chungking.js"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true
  }
}
