{
  "name": "@resir014/chungking-core",
  "version": "0.1.2-canary.3",
  "description": "Core theme variables for the Chungking Design System.",
  "type": "module",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "repository": "https://github.com/resir014/chungking.git",
  "author": "Resi Respati <resir014@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup-node",
    "clean": "rimraf dist",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "prepublishOnly": "yarn clean && yarn build",
    "type-check": "tsc",
    "validate": "yarn lint && yarn type-check"
  },
  "tsup": {
    "target": "node16",
    "entryPoints": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true
  }
}
