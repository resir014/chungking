{
  "name": "@resir014/chungking-core",
  "version": "0.1.2-canary.7",
  "description": "Core theme variables for the Chungking Design System.",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/resir014/chungking.git",
  "author": "Resi Respati <resir014@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup-node --legacy-output",
    "clean": "rimraf dist",
    "dev": "tsup-node --watch --legacy-output",
    "prepublish": "yarn clean && yarn build"
  },
  "tsup": {
    "target": "node16",
    "entryPoints": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true
  }
}
